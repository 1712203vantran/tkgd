 ko quá xuất sắc nhưng với mức giá này mình thấy ổn tôm nghêu tươi thoải mái còn thịt thì có hơi lâu nhưng mà thịt ở đây tươi ngon lắm ko phải vụng hay kém chất lượng 🥩🍤\n\nĐăc biệc ở đay kimchi ngon 😋 có cái nc rữa tay thơm thơm, vào hay ra gì củng dc cúi đầu chào 🥳 còn được cả bảo vệ ng Hàn Quốc dắt xe hộ mình 😍😍\n\n🕹 38 Hoa Mai phường 2 quận Phú Nhuận\n\n", like_count: 56, image_count: x, video_count: d, comment_count: d, bookmark_count: N, view_count: d, type: n, author_uuid: "d60fce284d654527bd1ade205635ca9f", list_topic_tag: [{ uuid: P, name: Q, alias_link: R }, { uuid: S, name: A, alias_link: A }], list_tag: [], list_media: [{ uuid: "8e85bf4d3fb149a9bef8fa7efde30f28", media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F02338b90d5e1b3234dd96f85c252f81c_output.jpeg", original_media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F02338b90d5e1b3234dd96f85c252f81c.jpeg", original_cover_link: a, video_thumb: a, width: h, height: c, cover_link: a, media_type: b }, { uuid: "fb68e09bd3fd4a4c995a94e511941855", media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F9e8a5fb1e17441b26b8763d3eddd2105_output.jpeg", original_media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F9e8a5fb1e17441b26b8763d3eddd2105.jpeg", original_cover_link: a, video_thumb: a, width: h, height: c, cover_link: a, media_type: b }, { uuid: "4cd117b40d5745fc897b946d35bb934c", media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F53089bfdd1159526533b06198cf20279_output.jpeg", original_media_link: "https:\u002F\u002Fstatic.riviu.co\u002Fimage\u002F2020\u002F11\u002F30\u002F53089bfdd1159526533b06198cf20279.jpeg", original_cover_link: a, video_thumb: a, width: h, height: c, cover_link: a, media_type: b }, { uuid: "43dd0666ed2d43e2b26fff5f8a950